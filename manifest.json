{
  "manifest_version": 2,
  "name": "Photophobia Dynamic",
  "short_name": "Photophobia",
  "description": "A theme that let you adjust colors shades and bright for texts and panels dynamically and easily.",
  "version": "1.3",
  "author": "Raphael Quintao",
  "icons": {
    "250": "assets/icon.png"
  },
  "permissions": [
    "theme",
    "storage"
  ],
  "background": {
    "page": "bg.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "250": "assets/icon.png"
    },
    "default_title": "Photophobia",
    "default_popup": "options.html"
  },
  "options_ui": {
    "browser_style": false,
    "page": "options.html"
  },
  "sidebar_action": {
    "default_title": "Photophobia",
    "default_panel": "sidebar.html",
    "default_icon": "assets/icon.png",
    "browser_style": false
  },
  "applications": {
    "gecko": {
      "id": "photophobia@quintao.ninja",
      "strict_min_version": "60.0"
    }
  }
}